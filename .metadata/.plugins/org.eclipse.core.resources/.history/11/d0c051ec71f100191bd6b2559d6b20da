      MasterReqB3         , 24;
      MasterReqB4         , 32;
      MasterReqB5         , 40;
      MasterReqB6         , 48;
      MasterReqB7         , 56;
   }
   SlaveResp           : 0x3d {
      SlaveRespB0         , 0;
      SlaveRespB1         , 8;
      SlaveRespB2         , 16;
      SlaveRespB3         , 24;
      SlaveRespB4         , 32;
      SlaveRespB5         , 40;
      SlaveRespB6         , 48;
      SlaveRespB7         , 56;
   }
}
/* ----------NODE ATTRIBUTE DEFINITIONS---------- */
Node_attributes {
   RLS {
      LIN_protocol               = "2.1";                    /* Node protocol version */
      configured_NAD             = 0x16;                     /* configured NAD of node (1-125) */
      initial_NAD                = 0x3;                      /* initial NAD of node (1-125) */
      product_id                 =    0x3,    0x3,   0xff;   /* Product id */
      response_error             = RLS_LIN_Error;            /* Response error signal */
      P2_min                     = 50 ms;                    /* P2_min */
      ST_min                     = 20 ms;                    /* ST_min */
      N_As_timeout               = 1000 ms;                  /* N_As timeout value */
      N_Cr_timeout               = 1000 ms;                  /* N_Cr timeout value */
      configurable_frames {
         BCM_01;
         BCM_02;
         RLS_01;
         RLS_02;
      }
   }
}
/* ----------SCHEDULE TABLE DEFINITIONS---------- */
Schedule_tables {
   NormalTable {
      RLS_01                              delay 10.0 ms;
      BCM_01                              delay 10.0 ms;
      BCM_02                              delay 10.0 ms;
      RLS_02                              delay 10.0 ms;
   }
   Diagnose {
      MasterReq                           delay 10.0 ms;
   }
}
/* ----------SIGNAL ENCODDING DEFINITIONS---------- */
Signal_encoding_types {
   RLS_HumidSensorValue_Encoding {
      physical_value,0,201,0.5,-0.5,"%";
   }
   RLS_TempSensorValue_Encoding {
      physical_value,0,254,0.5,-40.0,"deg";
   }
   RLS_TempDewPoint_Encoding {
      physical_value,0,1000,0.1,-39.6,"deg";
   }
}
/* ----------SIGNAL REPRESENTATION DEFINITIONS---------- */
Signal_representation {
   RLS_HumidSensorValue_Encoding:RLS_HumidSensorValue;
   RLS_TempDewPoint_Encoding:RLS_TempDewPoint;
   RLS_TempSensorValue_Encoding:RLS_TempSensorValue;
}

